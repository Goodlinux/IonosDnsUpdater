[
  {
    "type": "container",
    "title": "goodlinux/IonosDnsUpdater",
    "description": "Update Ionos DNS with IP from BOX",
    "categories": ["service"],
    "platform": "linux",
    "logo": "",
    "image": "goodlinux/ionosdnsupdater:latest",
    "env": [
      {
        "name": "API_KEY",
        "label": "Ionos API key"
      },
	  {
        "name": "DOMAIN",
        "label": "Domain to update"
      },
	  {
        "name": "VERBOSE",
        "label": "Verbose y/n"
      },
	  {
        "name": "BOX_IP",
        "label": "IP of the box"
      },
	  {
        "name": "BOX_USER",
        "label": "admin user name of the box"
      },
	  {
        "name": "BOX_PASSWORD",
        "label": "admin password of the box"
      },
	  {
        "name": "LOG_SRV",
        "label": "IP of the Log server"
      },
	  {
        "name": "HOSTNAME",
        "label": "Hostname of the container"
      },
	  {
        "name": "TZ",
        "label": "Time Zone ex : Europe/Paris"
      }
    ]
  }
]
